{
  "name": "@guardian/source-react-components-development-kitchen",
  "version": "0.0.5",
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/esm",
    "dist/cjs",
    "dist/types"
  ],
  "scripts": {
    "build": "npm-run-all clean --parallel build:*",
    "build:cjs": "tsc --composite false --declaration false --emitDeclarationOnly false --outDir dist/cjs --module commonjs",
    "build:esm": "tsc --composite false --declaration false --emitDeclarationOnly false --outDir dist/esm --module ES6",
    "build:types": "tsc -b",
    "clean": "rm -rf dist",
    "prepublish": "yarn build",
    "release": "np"
  },
  "dependencies": {
    "@guardian/src-button": "^3.9.0",
    "@guardian/src-helpers": "^3.9.0"
  },
  "devDependencies": {
    "@guardian/libs": "^3.2.1",
    "mini-svg-data-uri": "^1.3.3",
    "np": "^7.5.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "@emotion/react": "^11.1.2",
    "@guardian/libs": "^3.2.1",
    "@guardian/src-foundations": "^3.9.0",
    "@guardian/src-icons": "^3.9.0",
    "react": "^17.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
